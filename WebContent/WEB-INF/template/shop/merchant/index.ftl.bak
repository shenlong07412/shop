        <script>
            jQuery(document).ready(function() {
            var $areaId = $("#areaId");
            
            // 地区选择
			$areaId.lSelect({
				url: "${base}/common/area.jhtml"
			});
            
            
            var button1 = $("#logoUploadButton");
            var button2 = $("#avatarUploadButton");
			var exts = "jpg|png|jpeg|gif", paths = "|";
			new AjaxUpload(
				button1,
				{
					action : '${base}/payment/upload.jhtml',
					name : 'uploadFile',
					onSubmit : function(file, ext) {
						if (!RegExp("\.(?:" + exts + ")$$", "i").test(file)) {
							alert("只能上传以下类型：" + exts);
							return false;
						}
					},
					onComplete : function(file, response, statusText) {
						jsonData = eval("(" + response + ")");
						if (jsonData.rtnKey == 0) {
							alert(jsonData.rtnMsg);
						} else {
							document.getElementById("logo").value = jsonData.path;
							document.getElementById("logoFullPath").src = jsonData.fullPath;
							alert(jsonData.rtnMsg);
						}
					}
				});
				new AjaxUpload(
				button2,
				{
					action : '${base}/payment/upload.jhtml',
					name : 'uploadFile',
					onSubmit : function(file, ext) {
						if (!RegExp("\.(?:" + exts + ")$$", "i").test(file)) {
							alert("只能上传以下类型：" + exts);
							return false;
						}
					},
					onComplete : function(file, response, statusText) {
						jsonData = eval("(" + response + ")");
						if (jsonData.rtnKey == 0) {
							alert(jsonData.rtnMsg);
						} else {
							document.getElementById("avatar").value = jsonData.path;
							document.getElementById("avatarFullPath").src = jsonData.fullPath;
							alert(jsonData.rtnMsg);
						}
					}
				});
            
            	
            
            


                App.init();

                $('#tab1A').unbind("click");
                $('#tab2A').unbind("click");
                $('#tab3A').unbind("click");
                $('#tab4A').unbind("click");
                $('#tab5A').unbind("click");

                $('#username').on("blur",
                function() {
                    checkUsername("");
                });

                $('button[type=submit]').click(function() {
                    var result = checkUsername(""); // 校验用户名
                    if (!result) {
                        return;
                    }

                    var password = $('#password').val();
                    if (password == null || password.length == 0) {
                        alert("密码不能为空");
                        return;
                    } else if (password.length < 6) {
                        alert("密码长度不能少于6位");
                        return;
                    }

                    var confirmPassword = $('#confirmPassword').val();
                    if (confirmPassword == null || confirmPassword.length == 0) {
                        alert("再次输入密码不能为空");
                        return;
                    } else if (confirmPassword.length < 6) {
                        alert("再次输入密码长度不能少于6位");
                        return;
                    }
                    var realname = $('#realname').val();
                    if (realname == null || realname.length == 0) {
                        alert("商户中文名不能为空");
                        return;
                    }

                    if ($areaId == null || $areaId.length == 0) {
                        alert("社区为空");
                        return;
                    }

                    if (password != confirmPassword) {
                        $('#password').attr("value", "");
                        $('#confirmPassword').attr("value", "");
                        alert("密码与再次输入密码不一致，请重新输入");
                        return;
                    }

                    var json = {};
                    json.username = $('#username').val();
                    json.password = password;
                    json.realname = $('#realname').val();
                    json.nickname = $('#nickname').val();
                    json.avatar = $('#avatar').val();
                    json.areaId = $('#areaId').val();
                    json.bestpayAccount = $('#bestpayAccount').val();

                    var ivrAccount = $('#ivrAccount_1').val();
                    if (ivrAccount != '' && !ivrAccountCheck(ivrAccount, 1)) {
                        return false;
                    }
                    var allType = $("#ivrAccountTable input");
                    var index = 1;
                    for (var i = 1; i < allType.length; i++) {
                        var re = /\d+/;
                        elementID = allType[i].id.toString().match(re)[0];
                        if (document.getElementById('ivrAccount_' + elementID) != undefined && document.getElementById('ivrAccount_' + elementID).value != '') {
                            var ivrAccount_ = document.getElementById('ivrAccount_' + elementID).value;
                            if (!ivrAccountCheck(ivrAccount_, index)) {
                                return false;
                            }
                            ivrAccount += "|" + ivrAccount_;
                            index++;
                        }
                    }

                    json.ivrAccount = ivrAccount;
                    json.qrcode = $('#qrcode').val();
                    json.homePage = $('#homePage').val();
                    json.introduction = $('#introduction').val();
                    json.channelId = $('#channel').val();
                    json.templateType = $('#templateType').val();
                    json.profit = $('input[name=profit]:checked').val();
                    json.auditSwitch = $('input[name=auditSwitch]:checked').val();
                    json.pushFrequency = $('#pushFrequency').val();
                    json.billingType = $('input[name=billingType]:checked').val();
                    json.billingCycle = $('#billingCycle').val();
                    json.supportedRebate = $('input[name=supportedRebate]:checked').val();
                    json.royaltyRate = $('#royaltyRate').val();
                    json.productType = $('#productType').val();
                    json.feeType = $('input[name=feeType]:checked').val();
                    json.feeRatio = $('#feeRatio').val();
                    json.feeAmount = $('#feeAmount').val();
                    json.enterpriseName = $('#enterpriseName').val();
                    json.businessLicenseNo = $('#businessLicenseNo').val();
                    json.businessLicenseAddress = $('#businessLicenseAddress').val();
                    json.businessPeriod = $('#businessPeriod').val();
                    json.businessRange = $('#businessRange').val();
                    json.fax = $('#fax').val();
                    json.phone = $('#phone').val();
                    json.email = $('#email').val();
                    json.registeredCapital = $('#registeredCapital').val();
                    json.organizationCode = $('#organizationCode').val();
                    json.industry = $('#industry').val();
                    json.legalPersonRealname = $('#legalPersonRealname').val();
                    json.idCardNo = $('#idCardNo').val();
                    json.legalPersonPhone = $('#legalPersonPhone').val();
                    json.legalPersonAddress = $('#legalPersonAddress').val();
                    json.bankAccountName = $('#bankAccountName').val();
                    json.bankName = $('#bankName').val();
                    json.bankBranchName = $('#bankBranchName').val();
                    json.bankAddress = $('#bankAddress').val();
                    json.bankAccount = $('#bankAccount').val();
                    json.logo = $('#logo').val();
                    var data = JSON.stringify(json);

                    $.ajax({
                        type: "GET",
                        url: "${base}/shop/merchant/save.jhtml",
                        data: {"data":data},
                        contentType: "application/json",
                        dataType: "json",
                        success: function(msg) {
                            var result = msg.message;
                            // alert("here, msg: "+msg);
                            if (result.indexOf("注册成功") >= 0) {
                                alert(result);
                                document.location.href = "index.jsp";
                            } else {
                                alert(result);
                            }
                        },
                        error: function(msg) {
                            var result = msg.message;
                            // alert("here, msg: "+msg);
                            if (result.indexOf("注册成功") >= 0) {
                                alert(result);
                                document.location.href = "index.jsp";
                            } else {
                                alert(result);
                            }
                        }
                    });
                    return false;
                });

            });
        </script>
